{
  "name": "simple-react-full-stack",
  "version": "1.0.0",
  "description": "Boilerplate to build a full stack web application using React, Node.js, Express and Webpack.",
  "main": "src/server.ts",
  "scripts": {
    "test": "jest",
    "testcov": "jest --coverage",
    "bld": "webpack --mode production",
    "start": "npm run bld && node src/server.ts",
    "dev-": "nodemon -r dotenv/config src/server.ts dotenv_config_path=.env.development ",
    "dev": "tsc-watch --onSuccess \"node -r dotenv/config ./dist/server.js dotenv_config_path=.env.development\"",
    "dev--": "nodemon -r dotenv/config src/server.ts dotenv_config_path=.env.development "
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "html"
    ]
  },
